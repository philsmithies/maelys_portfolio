{
  "name": "simple-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && npx sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env.local ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@sanity/client": "^3.2.0",
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "framer-motion": "4.1.17",
    "lerna": "^3.22.1",
    "next": "12",
    "next-sanity": "^0.1.7",
    "next-sanity-image": "^3.2.1",
    "nodemailer": "^6.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.33.1",
    "react-json-pretty": "^2.2.0",
    "react-responsive-masonry": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
}
